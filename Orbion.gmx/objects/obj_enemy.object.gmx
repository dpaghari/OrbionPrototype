//change bullet speed based on current game time
var bulletSpeed = 8;
if (obj_HUDcontroller.alarm[1] &lt; 4*60*room_speed)
bulletSpeed = 10;
if (obj_HUDcontroller.alarm[1] &lt; 2*60*room_speed)
bulletSpeed = 12;
if (obj_HUDcontroller.alarm[1] &lt; 0.5*60*room_speed)
bulletSpeed = 13;

    
//shoot bullet at target
if(instance_exists(target)){
  if(!global.isDead){
    if(distance_to_object(target) &gt; 350){
        move_towards_point(target.x, target.y, 3);
    } else if(enemy_firing_timer &gt; enemy_rate){
        speed = 0;
        temp_bullet = instance_create(x, y, obj_enemy_bullet);
        temp_bullet.image_angle = point_direction(x, y, target.x, target.y);
        with(temp_bullet){move_towards_point(target.x, target.y, bulletSpeed);}
        enemy_firing_timer = 0;
    }
  }
}

enemy_firing_timer++;

image_speed = 0.3;
